<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FontableView">
        <attr name="fontType" format="enum">
            <enum name="MULI_REG" value="0"/>
            <enum name="MULI_LIGHT" value="1"/>
            <enum name="MULI_BOLD" value="2"/>
            <enum name="MULI_EX_BOLD" value="3"/>
            <enum name="MULI_SEMI_BOLD" value="4"/>

        </attr>
    </declare-styleable>

    <attr name="title" format="string" />

    <declare-styleable name="ValuesRangeViewValues">
        <attr name="title"/>
    </declare-styleable>

    <declare-styleable name="SeekBarViewValues">
        <attr name="title"/>
        <attr name="max" format="integer"/>
        <attr name="withInput" format="boolean"/>
        <attr name="inputType" format="string"/>
        <attr name="isDecimal" format="boolean"/>
    </declare-styleable>


</resources>

