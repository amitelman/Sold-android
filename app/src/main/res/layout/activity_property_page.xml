                                android:layout_gravity="center"
                                android:drawableLeft="@drawable/icon_tab_photos_selected_copy"
                                android:drawablePadding="6dp"
                                android:text="Photos"
                                android:textColor="@color/red_white_text_selector"
                                android:textSize="11.5sp" />

                        </FrameLayout>

                        <FrameLayout
                            android:id="@+id/propertyActMapViewBtnLayout"
                            android:layout_width="0dp"
                            android:layout_height="27.8dp"
                            android:layout_weight="1"
                            android:background="@drawable/right_radius_selector">

                            <sold.monkeytech.com.sold_android.ui.fontable_views.FontableTextView
                                android:id="@+id/propertyActMapViewBtn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:drawableLeft="@drawable/icon_tab_map_normal_copy"
                                android:drawablePadding="6dp"
                                android:text="Map View"
                                android:textColor="@color/red_white_text_selector"
                                android:textSize="11.5sp" />

                        </FrameLayout>

                    </LinearLayout>

                </android.support.design.widget.CollapsingToolbarLayout>
            </android.support.design.widget.AppBarLayout>

        </android.support.design.widget.CoordinatorLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="top"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="14.7dp"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/propertyActBackBtn"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:background="@null"
                android:padding="5dp"
                android:src="@drawable/back" />

            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_weight="1" />

            <ImageButton
                android:id="@+id/propertyActShareBtn"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:layout_marginRight="18.7dp"
                android:background="@null"
                android:src="@drawable/share" />

            <ImageButton
                android:id="@+id/propertyActFavoriteBtn"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:background="@null"
                android:src="@drawable/heart_full" />

        </LinearLayout>

    </FrameLayout>

</layout>